{
    "name": "spyck/automation-sonata-bundle",
    "description": "Automation Sonata Bundle for Symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Willem Verspyck",
            "email": "willemverspyck@outlook.com",
            "homepage": "https://verspyck.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "php",
        "sonata",
        "automation"
    ],
    "homepage": "https://github.com/willemverspyck/automation-sonata-bundle",
    "type": "library",
    "require": {
        "php": "^8.1",
        "sonata-project/admin-bundle": "^4",
        "sonata-project/doctrine-orm-admin-bundle": "^4",
        "sonata-project/form-extensions": "^2",
        "spyck/automation-bundle": "dev-main",
        "spyck/sonata-extension": "dev-main",
        "symfony/framework-bundle": "^6|^7"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3",
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Spyck\\AutomationSonataBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spyck\\AutomationSonataBundle\\Tests\\": "tests/"
        }
    }
}
